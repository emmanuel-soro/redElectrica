package prim;

public class Arista {
	private int nodoOrigen;

	public int getNodoOrigen() {
		return nodoOrigen;
	}

	public int getNodoDestino() {
		return nodoDestino;
	}

	public int getPeso() {
		return peso;
	}

	private int nodoDestino;
	private int peso;

	public Arista(int origen, int destino, int costo) {
		this.nodoOrigen = origen;
		this.nodoDestino = destino;
		this.peso = costo;
	}
}